<launch>
  <!--Pose format is: posx posy posz roll pitch yaw -->
  <arg name="box_pose" value="'-1 -1 0 0 0 0.785'"/>
  <arg name="iRobot_pose" value="'-1 0 0 0 0 0'"/>
  <arg name="iRobot_0_pose" value="'-1 -2 0 0 0 0'"/>
  <arg name="iRobot_1_pose" value="'-1 2 0 0 0 0'"/>
  
  <node name="onerobot_world_editor" required="true"  pkg="cooperative_transport" type="world_editor" args="$(find cooperative_transport) $(arg box_pose) $(arg iRobot_pose)" output="screen"/>

  <node name="severalrobots_world_editor" required="true"  pkg="cooperative_transport" type="world_editor" args="$(find cooperative_transport) $(arg box_pose) $(arg iRobot_pose) $(arg iRobot_0_pose) $(arg iRobot_1_pose) " output="screen"/>

</launch>